S_0x57b5eeaa8dc0 .scope module, "testbench" "testbench" 2 6;
 .timescale -9 -12;
v0x57b5eeaf6950_0 .var "data", 7 0;
S_0x57b5eeaa8f50 .scope module, "DUT" "bit_vector_sum" 2 10, 3 1 0, S_0x57b5eeaa8dc0;
 .timescale -9 -12;
    .port_info 0 /INPUT 8 "data";
    .port_info 1 /OUTPUT 4 "sum";
P_0x57b5eeace540 .param/l "DATA_W" 0 3 3, +C4<00000000000000000000000000001000>;
P_0x57b5eeace580 .param/l "POS_W" 0 3 4, +C4<00000000000000000000000000000011>;
v0x57b5eeaf6710_0 .net "data", 7 0, v0x57b5eeaf6950_0;  1 drivers
v0x57b5eeaf6810_0 .net "sum", 3 0, L_0x57b5eeaf6a40;  1 drivers
L_0x57b5eeaf6a40 .ufunc/vec4 TD_testbench.DUT.bit_sum, 4, v0x57b5eeaf6950_0 (v0x57b5eeaf6540_0) S_0x57b5eeae2c10;
S_0x57b5eeae2c10 .scope function.vec4.s4, "bit_sum" "bit_sum" 3 25, 3 25 0, S_0x57b5eeaa8f50;
 .timescale -9 -12;
; Variable bit_sum is vec4 return value of scope S_0x57b5eeae2c10
v0x57b5eeaf6540_0 .var "data_in", 7 0;
v0x57b5eeaf6620_0 .var/i "i", 31 0;
TD_testbench.DUT.bit_sum ;
    %pushi/vec4 0, 0, 4;
    %ret/vec4 0, 0, 4;  Assign to bit_sum (store_vec4_to_lval)
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x57b5eeaf6620_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x57b5eeaf6620_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %retload/vec4 0; Load bit_sum (draw_signal_vec4)
    %load/vec4 v0x57b5eeaf6540_0;
    %load/vec4 v0x57b5eeaf6620_0;
    %part/s 1;
    %pad/u 4;
    %add;
    %cassign/vec4 v0x57b5eeae2df0_0;
    %load/vec4 v0x57b5eeaf6620_0;
    %addi 1, 0, 32;
    %store/vec4 v0x57b5eeaf6620_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x57b5eeaa8dc0;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "out.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57b5eeaa8dc0 {0 0 0};
    %delay 10000, 0;
    %pushi/vec4 17, 0, 8;
    %store/vec4 v0x57b5eeaf6950_0, 0, 8;
    %delay 10000, 0;
    %pushi/vec4 102, 0, 8;
    %store/vec4 v0x57b5eeaf6950_0, 0, 8;
    %delay 10000, 0;
    %pushi/vec4 200, 0, 8;
    %store/vec4 v0x57b5eeaf6950_0, 0, 8;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x57b5eeaf6950_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
